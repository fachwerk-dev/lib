{
  "name": "fachwerk",
  "version": "0.0.11",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fachwerk-dev/fachwerk.git"
  },
  "homepage": "https://github.com/fachwerk-dev/fachwerk",
  "keywords": [
    "vite",
    "vue",
    "vue-component"
  ],
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/fachwerk.cjs",
  "module": "./dist/fachwerk.mjs",
  "exports": {
    ".": {
      "import": "./dist/fachwerk.mjs",
      "require": "./dist/fachwerk.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "typings": "./dist/lib.esm.ts",
  "scripts": {
    "dev": "vite --port 3000 --host -c vite.config.app.ts",
    "build": "vite build -c vite.config.esm.ts &&vite build -c vite.config.cjs.ts && vite build -c vite.config.app.ts",
    "preview": "vite preview --port 5000",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "release": "vitest --run && npm run build && vue-tsc --declaration --emitDeclarationOnly"
  },
  "dependencies": {
    "d3-shape": "^3.1.0",
    "katex": "^0.15.2",
    "mitt": "^3.0.0",
    "pdf-lib": "^1.17.1",
    "svgpath": "^2.5.0",
    "three": "^0.137.5",
    "vue": "^3.2.30"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.0.12",
    "@tailwindcss/typography": "^0.5.1",
    "@types/d3-shape": "^3.0.2",
    "@types/katex": "^0.11.1",
    "@types/node": "^17.0.17",
    "@types/react": "file:src/types",
    "@types/three": "^0.137.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vueuse/core": "^7.6.1",
    "gray-matter": "^4.0.3",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "tailwindcss": "^3.0.21",
    "unplugin-icons": "^0.13.1",
    "vite": "^2.8.1",
    "vite-plugin-fonts": "^0.2.2",
    "vite-plugin-md": "^0.11.8",
    "vite-plugin-pages": "^0.20.2",
    "vitepress": "^0.22.0",
    "vitest": "^0.3.2",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.31.2"
  }
}
