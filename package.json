{
  "name": "fachwerk",
  "version": "0.0.13",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fachwerk-dev/fachwerk.git"
  },
  "homepage": "https://github.com/fachwerk-dev/fachwerk",
  "keywords": [
    "vite",
    "vue",
    "vue-component"
  ],
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/fachwerk.cjs",
  "module": "./dist/fachwerk.mjs",
  "exports": {
    ".": {
      "import": "./dist/fachwerk.mjs",
      "require": "./dist/fachwerk.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "typings": "./dist/lib.esm.ts",
  "scripts": {
    "dev": "vite --port 3000 --host -c vite.config.app.ts --open",
    "build": "vite build -c vite.config.esm.ts && vite build -c vite.config.cjs.ts && vite build -c vite.config.app.ts",
    "preview": "vite preview --port 5000",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "release": "vitest --run && npm run build && vue-tsc --declaration --emitDeclarationOnly",
    "bump": "bumpp --commit \"chore: v%s\""
  },
  "dependencies": {
    "d3-shape": "^3.1.0",
    "katex": "^0.15.3",
    "mitt": "^3.0.0",
    "pdf-lib": "^1.17.1",
    "svgpath": "^2.5.0",
    "three": "^0.138.3",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.1.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/d3-shape": "^3.0.2",
    "@types/katex": "^0.11.1",
    "@types/node": "^17.0.22",
    "@types/react": "file:src/types",
    "@types/three": "^0.138.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vueuse/core": "^8.1.2",
    "bumpp": "^7.1.1",
    "gray-matter": "^4.0.3",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "unplugin-icons": "^0.14.0",
    "vite": "^2.8.6",
    "vite-plugin-fonts": "^0.3.0",
    "vite-plugin-md": "^0.11.7",
    "vite-plugin-pages": "^0.22.0",
    "vitepress": "^0.22.3",
    "vitest": "^0.7.7",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.33.6"
  }
}
